{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.shoun-llc.jp" }],
      "destination": "https://shoun-llc.jp/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/privacy.html",
      "has": [
        { "type": "host", "value": "privacy.shoun-llc.jp" }
      ]
    }
  ]
}
